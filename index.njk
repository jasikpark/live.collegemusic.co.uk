{% extends "_includes/default.njk" %}

{% block title %}{% this_is_dev_server %}College Music{% endblock %}

{% block content %}
    <div class="sm:my-4 sm:rounded-lg space-y-4 framing">
        <header class="flex flex-wrap justify-between w-full pt-2 sm:pt-8 px-2 sm:px-8 relative">
            <a href="#content" class="sr-only focus:not-sr-only focus:text-white focus:absolute focus:top-0 focus:right-0">
                Skip to content
            </a>
            <h1 class="text-white inline">College Music |
                <span class="font-semibold">Lofi Hip Hop Radio</span></h1>
            <span>
                <span x-data="initClock()" x-init="startClock">
                    <time x-text="time" class="text-white inline"></time>
                </span>
                <span x-data="initWeather()" x-init="getWeather2">
                    <img aria-hidden="true" :src="weather_icon" :title="weather_description" class="inline h-8 drop-shadow">
                    <span class="sr-only focus:not-sr-only" x-text="weather_description"></span>
                </span>
            </span>
        </header>

        <div class="hello-container space-y-4 lg:space-y-0 px-2 sm:px-8">
            <div class="">
                <div class="video-container">
                    <iframe title="Youtube video radio stream" class="rounded-sm" width="853" height="480" src="https://www.youtube-nocookie.com/embed/MCkTebktHVc" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen="allowfullscreen"></iframe>
                </div>
            </div>
            <div class="hidden sm:block">
                <div class="chat-container">
                    <iframe title="Youtube live chat" class="rounded-sm" width="480" height="480" referrerpolicy="origin" src="https://www.youtube.com/live_chat?v=MCkTebktHVc&embed_domain={% hostname %}&dark_theme=1" allow="autoplay; encrypted-media" id="chat-embed" frameborder="0"></iframe>
                </div>
            </div>
        </div>

        <div id="content" class="flex flex-col space-y-4 lg:flex-row lg:justify-between lg:space-y-0 lg:space-x-8 pb-8 px-2 sm:px-8">
            <div x-data="initSongData()" x-init="getSongData" class="grid grid-flow-row gap-4">
                <div class="rounded-lg flex align-center now-playing">
                    <img :src="songData.now_playing.song.art" class="rounded-md shadow-lg now-playing-album-art" aria-labelledby="now-playing-song-details" alt="" src="/static/default_album_art.png"/>
                    <div class="flex flex-wrap w-full justify-center space-x-4 align-center p-3 sm:pr-4 pl-32">
                        <button :title="playing ? 'playing' : 'paused'" :aria-label="playing ? 'playing' : 'paused'" @click.debounce="playing = !playing">
                            <svg aria-label="paused" :class="{ 'hidden': playing, 'block': !playing }" class="block drop-shadow w-8 h-8" width="30" height="35" viewbox="0 0 30 35" fill="none" xmlns="http://www.w3.org/2000/svg">
                                <g clip-path="url(#clip0)">
                                    <path d="M26.8704 22.4705L10.1135 33.7965C6.0304 36.5569 0.5 33.6571 0.5 28.7611V6.10612C0.5 1.20727 6.0304 -1.68962 10.1135 1.0707L26.8704 12.3968C30.4508 14.8171 30.4508 20.0501 26.8704 22.4705Z" fill="black"/>
                                </g>
                                <path d="M26.3704 22.4705L9.61354 33.7965C5.5304 36.5569 0 33.6571 0 28.7611V6.10612C0 1.20727 5.5304 -1.68962 9.61354 1.0707L26.3704 12.3968C29.9508 14.8171 29.9508 20.0501 26.3704 22.4705Z" fill="url(#paint0_linear)"/>
                                <defs>
                                    <lineargradient id="paint0_linear" x1="-11.729" y1="-6.29059" x2="-11.729" y2="28.5762" gradientunits="userSpaceOnUse">
                                        <stop stop-color="#4F4F4F"/>
                                        <stop offset="1" stop-color="white" stop-opacity="0"/>
                                    </lineargradient>
                                    <clippath id="clip0">
                                        <rect width="29.0557" height="34.8668" fill="white" transform="translate(0.5)"/>
                                    </clippath>
                                </defs>
                            </svg>

                            <svg aria-label="playing" :class="{ 'hidden': !playing, 'block': playing }" class="hidden drop-shadow w-8 h-8" width="27" height="27" viewbox="0 0 27 27" fill="none" xmlns="http://www.w3.org/2000/svg">
                                <path fill-rule="evenodd" clip-rule="evenodd" d="M8.87504 26.0557H3.29168C1.75044 26.0557 0.5 24.8052 0.5 23.264V2.79168C0.5 1.25044 1.75044 0 3.29168 0H8.87504C10.4163 0 11.6667 1.25044 11.6667 2.79168V23.264C11.6667 24.8052 10.4163 26.0557 8.87504 26.0557ZM26.5557 23.264V2.79168C26.5557 1.25044 25.3052 0 23.764 0H18.1806C16.6394 0 15.3889 1.25044 15.3889 2.79168V23.264C15.3889 24.8052 16.6394 26.0557 18.1806 26.0557H23.764C25.3052 26.0557 26.5557 24.8052 26.5557 23.264Z" fill="url(#paint1_linear)"/>
