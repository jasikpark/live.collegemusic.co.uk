    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@11ty/eleventy": "0.11.1",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@ryangjchandler/spruce": "2.2.2",
    "alpinejs": "2.7.3",
    "env-var": "7.0.0",
    "html-minifier": "4.0.0",
    "husky": "4.3.4",
    "postcss-clean": "1.1.0",
    "postcss-cli": "8.3.0",
    "tailwindcss": "1.9.6",
    "tldjs": "2.3.1"
  },
  "dependencies": {}
}
