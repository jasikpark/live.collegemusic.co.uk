{
  "name": "live-collegemusic-co-uk",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:jasikpark/live.collegemusic.co.uk.git",
  "author": "Caleb Jasik <calebjasik@jasik.xyz>",
  "license": "MIT",
  "scripts": {
    "start": "BASE_URL='http://localhost:8080' eleventy --serve & postcss assets/css/tailwind.css --o _tmp/style.css --watch & esbuild assets/js/site.js --bundle --watch --outfile='_tmp/site.js'",
    "build": "ELEVENTY_PRODUCTION=true eleventy & NODE_ENV=production postcss assets/css/tailwind.css --o _site/style.css & esbuild site.js --bundle --minify --outfile='_site/site.js'"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@11ty/eleventy": "0.11.1",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "autoprefixer": "10.2.4",
    "cssnano": "4.1.10",
    "env-var": "7.0.0",
    "esbuild": "0.8.43",
    "html-minifier": "4.0.0",
    "husky": "4.3.8",
    "postcss": "8.2.4",
    "postcss-cli": "8.3.1",
    "postcss-focus-visible": "5.0.0",
    "prettier": "2.2.1",
    "prettier-plugin-toml": "0.3.1",
    "stylelint": "13.9.0",
    "stylelint-config-standard": "20.0.0",
    "tldjs": "2.3.1"
  },
  "dependencies": {
    "@ryangjchandler/spruce": "2.6.3",
    "@tailwindcss/aspect-ratio": "0.2.0",
    "alpinejs": "2.8.0",
    "focus-visible": "5.2.0",
    "mnemonist": "0.38.1",
    "tailwind-easing-gradients": "1.0.2",
    "tailwindcss": "2.0.2"
  }
}
